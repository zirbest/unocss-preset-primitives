{
  "name": "unocss-preset-primitives",
  "type": "module",
  "version": "0.0.2-beta.0",
  "packageManager": "pnpm@8.12.0",
  "description": "A UnoCSS preset and variants for styling headlessui, radix-ui, kobalte state or custom one",
  "author": "zirbest <zirbst@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/zirbest",
  "homepage": "https://github.com/zirbest/unocss-preset-primitives#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zirbest/unocss-preset-primitives.git"
  },
  "bugs": "https://github.com/zirbest/unocss-preset-primitives/issues",
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-primitives",
    "preset",
    "primitives",
    "headlessui",
    "radix-ui",
    "kobalte"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@unocss/core": "^0.58.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.3",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.10.4",
    "@unocss/autocomplete": "0.59.0-beta.1",
    "@vitest/ui": "^1.0.4",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "esno": "^4.0.0",
    "pnpm": "^8.12.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.7",
    "vitest": "^1.0.4"
  }
}
